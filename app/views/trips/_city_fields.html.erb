<li class="city">
<div class="row">
    <div class="small-2 large-6 columns">
      <%= f.label :name, "City" %>
      <%= f.text_field :name %>
    </div>
    <div class="small-2 large-6 columns">
      <%= f.label :country %>
      <%= f.text_field :country %>
    <div>

</div>
<h3 class="title-box">Trip / City / Itenerary</h3>

      <div class="nested-level-2">
        <ul class="iteneraries-container">
          <%= f.nested_fields_for :iteneraries, new_item_index: 'new_itenerary_item', html: { class: "itenerary-form" } do |f| %>
          <div class="row">
            <li class="itenerary">
              <div class="small-2 large-3 columns">
                <%= f.label :item %>
                <%= f.text_field :item %>
              </div>
              <div class="small-2 large-3 columns">
                <%= f.check_box :activity_to_do %>
                <%= f.label :activity_to_do %>
              </div>
              <div class="small-2 large-3 columns">
                <%= f.check_box :place_to_eat %>
                <%= f.label :place_to_eat %>
              </div>
              <div class="small-2 large-3 columns">
                <%= f.check_box :place_to_sleep %>
                <%= f.label :place_to_sleep %>
              </div>

              <a href="#" class="itenerary-remove">remove</a>

              <%= f.hidden_field :id %>
              <%= f.hidden_field :_destroy %>
              <hr />
            </li>
          </div>
          <% end %>
        </ul>
        <a href="#" class="itenerary-add">Add Itenerary</a>
    </div>
  <a href="#" class="itenerary-remove">remove</a>
  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</li>
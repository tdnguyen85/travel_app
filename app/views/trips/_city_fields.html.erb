    <div class="small-2 large-6 columns">
      <%= f.label :name, "City" %>
      <%= f.text_field :name %>
    </div>
    <div class="small-2 large-6 columns">
      <%= f.label :country %>
      <%= f.text_field :country %>
    <div>

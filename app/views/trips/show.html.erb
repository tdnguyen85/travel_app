<div class="container">
  <p id="notice"><%= notice %></p>

  <p>
    <b>Title:</b>
    <%= @trip.title %>
  </p>

  <p>
    <b>User:</b>
    <%= @trip.user_id %>
  </p>

  <p>
    <b>Like:</b>
    <%= @trip.like %>
  </p>

  <p>
    <b>Begin on:</b>
    <%= @trip.begin_on %>
  </p>

  <p>
    <b>End on:</b>
    <%= @trip.end_on %>
  </p>
  <p>
  <p>
    <%= @trip_cities.inspect %><br />
    <%= @cities_in_current_trip.inspect %><br />
    <%= @cities_in_current_trip.name %><br />
    <%= @iteneraries_of_current_trip.inspect %>
  </p>
  <% @cities_in_current_trip.each do |i| %>
    <%= i.name %><br />
  <% end %>
  <% @iteneraries_of_current_trip.each do |i| %>
    <%= i.item %> <br />
  <% end %>
  <%= link_to 'Edit', edit_trip_path(@trip) %> |
  <%= link_to 'Back', trips_path %>
</div>